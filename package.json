{
  "name": "api-rest",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jessica Bandeira",
  "license": "MIT",
  "scripts": {
    "start": "yarn ts-node-dev ./src/index.ts",
    "production": "node ./build/index.js",
    "postinstall": "tsc",
    "knex:rollback-all": "knex --knexfile ./src/server/database/knex/Environments.ts migrate:rollback --all",
    "knex:rollback": "knex --knexfile ./src/server/database/knex/Environments.ts migrate:rollback",
    "knex:migrate": "knex --knexfile ./src/server/database/knex/Environments.ts migrate:latest",
    "knex:seed": "knex --knexfile ./src/server/database/knex/Environments.ts seed:run",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.6.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "engines": {
    "node": "20.x"
  }
}
